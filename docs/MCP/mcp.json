{
    "mcpServers": {
      "browser-tools": {
        "command": "npx",
        "args": [
          "@agentdeskai/browser-tools-mcp@1.2.0"
        ],
        "enabled": true
      },
      "github": {
        "command": "npx",
        "args": [
          "-y",
          "@modelcontextprotocol/server-github"
        ],
        "env": {
          "GITHUB_PERSONAL_ACCESS_TOKEN": "---"
        }
      },
      "@magicuidesign/mcp": {
        "command": "npx",
        "args": [
          "-y",
          "@magicuidesign/mcp@latest"
        ]
      }
    }
  }